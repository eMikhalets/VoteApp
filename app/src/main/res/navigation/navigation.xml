<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.supercasual.fourtop.fragment.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
        <action
            android:id="@+id/action_loginFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_loginFragment_to_userImagesFragment"
            app:destination="@id/userImagesFragment" />
        <action
            android:id="@+id/action_loginFragment_to_votingFragment"
            app:destination="@id/votingFragment" />
        <action
            android:id="@+id/action_loginFragment_to_topImagesFragment"
            app:destination="@id/topImagesFragment" />
        <action
            android:id="@+id/action_loginFragment_to_topUsersFragment"
            app:destination="@id/topUsersFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.supercasual.fourtop.fragment.RegisterFragment"
        android:label="RegisterFragment" />
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.supercasual.fourtop.fragment.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.supercasual.fourtop.fragment.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/topImagesFragment"
        android:name="com.supercasual.fourtop.fragment.TopImagesFragment"
        android:label="TopImagesFragment" />
    <fragment
        android:id="@+id/topUsersFragment"
        android:name="com.supercasual.fourtop.fragment.TopUsersFragment"
        android:label="TopUsersFragment" />
    <fragment
        android:id="@+id/userImagesFragment"
        android:name="com.supercasual.fourtop.fragment.UserImagesFragment"
        android:label="UserImagesFragment" />
    <fragment
        android:id="@+id/votingFragment"
        android:name="com.supercasual.fourtop.fragment.VotingFragment"
        android:label="VotingFragment" />
</navigation>